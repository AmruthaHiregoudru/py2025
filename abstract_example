from abstract_class import Vechile
class Bike(Vechile):
    def __init__(self,n,color):
        super().__init__(n)
        self.color=color
    def start(self):
        print("Start with kick")
class Scooty(Vechile):
    def start(self):
        print("Self start")
class Car(Vechile):
    def __init__(self,n,x):
        super.__init__(n)
        self.no_of_gears=6
    def start(self):
        print("Start with the key")